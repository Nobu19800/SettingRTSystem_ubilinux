# ADC_MCP3208 CMake config file
#
# This file sets the following variables:
# ADC_MCP3208_FOUND - Always TRUE.
# ADC_MCP3208_INCLUDE_DIRS - Directories containing the ADC_MCP3208 include files.
# ADC_MCP3208_IDL_DIRS - Directories containing the ADC_MCP3208 IDL files.
# ADC_MCP3208_LIBRARIES - Libraries needed to use ADC_MCP3208.
# ADC_MCP3208_DEFINITIONS - Compiler flags for ADC_MCP3208.
# ADC_MCP3208_VERSION - The version of ADC_MCP3208 found.
# ADC_MCP3208_VERSION_MAJOR - The major version of ADC_MCP3208 found.
# ADC_MCP3208_VERSION_MINOR - The minor version of ADC_MCP3208 found.
# ADC_MCP3208_VERSION_REVISION - The revision version of ADC_MCP3208 found.
# ADC_MCP3208_VERSION_CANDIDATE - The candidate version of ADC_MCP3208 found.

message(STATUS "Found ADC_MCP3208-@PROJECT_VERSION@")
set(ADC_MCP3208_FOUND TRUE)

find_package(<dependency> REQUIRED)

#set(ADC_MCP3208_INCLUDE_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@"
#    ${<dependency>_INCLUDE_DIRS}
#    )
#
#set(ADC_MCP3208_IDL_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@/idl")
set(ADC_MCP3208_INCLUDE_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@"
    ${<dependency>_INCLUDE_DIRS}
    )
set(ADC_MCP3208_IDL_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@/idl")


if(WIN32)
    set(ADC_MCP3208_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_STATIC_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
else(WIN32)
    set(ADC_MCP3208_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_SHARED_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
endif(WIN32)

set(ADC_MCP3208_DEFINITIONS ${<dependency>_DEFINITIONS})

set(ADC_MCP3208_VERSION @PROJECT_VERSION@)
set(ADC_MCP3208_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
set(ADC_MCP3208_VERSION_MINOR @PROJECT_VERSION_MINOR@)
set(ADC_MCP3208_VERSION_REVISION @PROJECT_VERSION_REVISION@)
set(ADC_MCP3208_VERSION_CANDIDATE @PROJECT_VERSION_CANDIDATE@)

