# ADC_MCP3002 CMake config file
#
# This file sets the following variables:
# ADC_MCP3002_FOUND - Always TRUE.
# ADC_MCP3002_INCLUDE_DIRS - Directories containing the ADC_MCP3002 include files.
# ADC_MCP3002_IDL_DIRS - Directories containing the ADC_MCP3002 IDL files.
# ADC_MCP3002_LIBRARIES - Libraries needed to use ADC_MCP3002.
# ADC_MCP3002_DEFINITIONS - Compiler flags for ADC_MCP3002.
# ADC_MCP3002_VERSION - The version of ADC_MCP3002 found.
# ADC_MCP3002_VERSION_MAJOR - The major version of ADC_MCP3002 found.
# ADC_MCP3002_VERSION_MINOR - The minor version of ADC_MCP3002 found.
# ADC_MCP3002_VERSION_REVISION - The revision version of ADC_MCP3002 found.
# ADC_MCP3002_VERSION_CANDIDATE - The candidate version of ADC_MCP3002 found.

message(STATUS "Found ADC_MCP3002-@PROJECT_VERSION@")
set(ADC_MCP3002_FOUND TRUE)

find_package(<dependency> REQUIRED)

#set(ADC_MCP3002_INCLUDE_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@"
#    ${<dependency>_INCLUDE_DIRS}
#    )
#
#set(ADC_MCP3002_IDL_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@/idl")
set(ADC_MCP3002_INCLUDE_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@"
    ${<dependency>_INCLUDE_DIRS}
    )
set(ADC_MCP3002_IDL_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@/idl")


if(WIN32)
    set(ADC_MCP3002_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_STATIC_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
else(WIN32)
    set(ADC_MCP3002_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_SHARED_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
endif(WIN32)

set(ADC_MCP3002_DEFINITIONS ${<dependency>_DEFINITIONS})

set(ADC_MCP3002_VERSION @PROJECT_VERSION@)
set(ADC_MCP3002_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
set(ADC_MCP3002_VERSION_MINOR @PROJECT_VERSION_MINOR@)
set(ADC_MCP3002_VERSION_REVISION @PROJECT_VERSION_REVISION@)
set(ADC_MCP3002_VERSION_CANDIDATE @PROJECT_VERSION_CANDIDATE@)

